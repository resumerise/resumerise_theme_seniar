<div class="key-list-item" id="<%= it.id %>">
    <% if(it.label) { %>
        <label class="label">
            <%= it.label %>
        </label>
    <% } %>
    <% if(it.data?.length) { %>
        <ul class="list">
        <% it.data.forEach(function(item) { %>
            <li class="list__item"><%= item %></li>
        <% }) %>
		</ul>
    <% } %>
</div>